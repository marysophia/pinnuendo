<% @pins.each do |pin| %>

  <%= link_to image_tag(pin.image.url(:medium)), pin %><br/>
  <%= pin.description %><br/>
  <%= pin.user.email if pin.user %><br/>
  
  <% if pin.user == current_user %>
    <%= link_to 'Edit', edit_pin_path(pin) %>
    <%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'You sure about that?' } %>
  <% end %>
  
<% end %>
